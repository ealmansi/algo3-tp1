#include "problema2.h"

using namespace std;
using namespace Problema2;

int main(){

	Pieza p1(1,3,1);
	Pieza p2(2,2,1);
	Pieza p3(3,2,1);
	Pieza p4(4,1,1);
	Pieza p5(5,1,1);

	vector<Pieza> vec;

	vec.push_back(p1);
	vec.push_back(p2);
	vec.push_back(p3);
	vec.push_back(p4);
	vec.push_back(p5);

	Salida s;
	s.piezas 0 vec;
	s.perdida_total = 10;

	escribir_salida(s);

	return 0;
}
